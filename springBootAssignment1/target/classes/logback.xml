<?xml version="1.0" encoding="UTF-8"?>
<configuration>

   <appender name="ROLLIN" class="ch.qos.logback.core.FileAppender">
    <file>C:/Users/Administrator/FseLog/logFile.log</file>
    
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="info">
    <appender-ref ref="ROLLIN"/>
  </root>

</configuration>